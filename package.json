{
  "name": "gw2data_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod": "concurrently  -n \"vuecli,server\" -c \"bgBlue.bold,bgGreen.bold\"  \"bnr client_prod\" \"bnr server_prod\"",
    "dev": "concurrently  -n \"vuecli,server\" -c \"bgBlue.bold,bgGreen.bold\"  \"bnr client_dev\" \"bnr server_dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "client_prod": {
      "command": "cd client&npm i&npm run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "server_prod": {
      "command": "cd server&npm i&npm start",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "client_dev": {
      "command": "cd client&npm run serve",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "server_dev": {
      "command": "cd server&npm run dev",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShinyMeta/gw2data_app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShinyMeta/gw2data_app/issues"
  },
  "homepage": "https://github.com/ShinyMeta/gw2data_app#readme",
  "devDependencies": {
    "better-npm-run": "^0.1.1",
    "concurrently": "^6.0.0"
  }
}
